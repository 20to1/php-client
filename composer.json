{
  "name": "20to1/php-client",
  "version": "1.0",
  "description": "PHP client library",
  "keywords": [
    "php",
    "sdk",
    "api"
  ],
  "homepage": "https://github.com/20to1/php-client",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Offchaindata\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "php": ">=7.2",
    "phpunit/phpunit": "^8.0"
  }
}
